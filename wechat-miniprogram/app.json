{
  "pages": [
    "pages/index/index",
    "pages/connection/connection",
    "pages/remote-desktop/remote-desktop",
    "pages/file-transfer/file-transfer",
    "pages/settings/settings",
    "pages/login/login"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#2196F3",
    "navigationBarTitleText": "工一远程",
    "navigationBarTextStyle": "white",
    "backgroundColor": "#f5f5f5",
    "enablePullDownRefresh": false
  },
  "tabBar": {
    "color": "#666666",
    "selectedColor": "#2196F3",
    "backgroundColor": "#ffffff",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/connection/connection",
        "text": "连接"
      },
      {
        "pagePath": "pages/file-transfer/file-transfer",
        "text": "文件"
      },
      {
        "pagePath": "pages/settings/settings",
        "text": "设置"
      }
    ]
  },
  "permission": {
    "scope.camera": {
      "desc": "用于屏幕共享和视频传输功能"
    },
    "scope.record": {
      "desc": "用于音频传输功能"
    },
    "scope.writePhotosAlbum": {
      "desc": "用于保存截图和传输的文件"
    },
    "scope.userLocation": {
      "desc": "用于网络诊断和连接优化"
    }
  },
  "requiredPrivateInfos": [
    "chooseImage",
    "chooseVideo",
    "chooseMessageFile"
  ],
  "requiredBackgroundModes": ["audio", "voip"],
  "networkTimeout": {
    "request": 10000,
    "downloadFile": 60000,
    "uploadFile": 60000,
    "connectSocket": 10000
  },
  "plugins": {},
  "resizable": true,
  "debug": false,
  "lazyCodeLoading": "requiredComponents"
}